echo call: F "Shop:"
Call Button [5] [7] [8F] [""]
GetInput /M %_Box% /H %_hover:-0, -3% 70
echo call: F "Scary Face" 100 TRbx 500 TBuxs
Call Button [5] [7] [8F] [""]
GetInput /M %_Box% /H %_hover:-0, -3% 70
echo call: F "Rich Face" 1000 TRbx 5000 TBuxs
If %errorlevel%==1 goto Games1
If %Errorlevel%==2 goto Shop1
If %Errorlevel%==3 goto Trading1
If %Errorlevel%==4 goto Casino1
If %Errorlevel%==5 goto ScaryFaceChose1
If %Errorlevel%==6 goto RichFace1



