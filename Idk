If %Errorlevel%==3 goto Trading1
If %Errorlevel%==4 goto Casino1
If %Errorlevel%==5 goto BuyScaryFace1


:RichFace1
@echo off
cls
Color 80
Call Button [3] [3] [8F] ["Games"]
GetInput /M %_Box% /H %_hover:-0, -3% 70
Call Button [3] [3] [8F] ["Shop"] GetInput /M %_Box% /H %_hover:-0, -3% 70
Call Button [3] [3] [8F] ["Trading"]
GetInput /M %_Box% /H %_hover:-0, -3% 70
Call Button [3] [3] [8F] ["Casino"]
GetInput /M %_Box% /H %_hover:-0, -3% 70
echo call: F "Coming Soon"


If %errorlevel%==1 goto Games1
If %Errorlevel%==2 goto Shop1
If %Errorlevel%==3 goto Trading1
If %Errorlevel%==4 goto Casino1


:BuyScaryFace1
@echo off
cls
Color 80
Call Button [3] [3] [8F] ["Games"]
GetInput /M %_Box% /H %_hover:-0, -3% 70
Call Button [3] [3] [8F] ["Shop"] GetInput /M %_Box% /H %_hover:-0, -3% 70
Call Button [3] [3] [8F] ["Trading"]
GetInput /M %_Box% /H %_hover:-0, -3% 70
Call Button [3] [3] [8F] ["Casino"]
GetInput /M %_Box% /H %_hover:-0, -3% 70
echo.
echo call: F "______________________________"
echo call: F "|                                                        |"
echo call: F "|                                                        |"
echo call: F "|                                                        |"
echo call: F "|                                                        |"
echo call: F "|                                                        |"
echo call: F "|                                                        |"
echo call: F "|                                                        |"
echo call: F "______________________________"
echo call: F "|     Call Button [3] [3] [8F] ["Cancel"]
GetInput /M %_Box% /H %_hover:-0, -3% 70 Call Button [3] [3] [8F] ["Buy"]
GetInput /M %_Box% /H %_hover:-0, -3% 70                                                   |"
echo call: F "______________________________"
If %errorlevel%==1 goto Games1
If %Errorlevel%==2 goto Shop1
If %Errorlevel%==3 goto Trading1
If %Errorlevel%==4 goto Casino1
If %Errorlevel%==5 goto ScaryFace1
If %Errorlevel%==6 goto  TRbxOrTBuxsScaryFace1


:BuyScaryFaceTBux1
If not Bbuxs == leq 500 goto BuyFail1
If not TBuxs == lss 500 goto  ScaryFaceCalculation1

:BuyScaryFaceTRbx1
If not TRbxs == leq 500 goto BuyFail1
If not TRbxs == lss 100 goto ScaryFaceCalculation2

:ScaryFaceCalculation2
set /a TRbxs-==100
set / a ScaryFaceBuyed+=1 
goto ScaryFaceOwned1

:ScaryFaceCalculation1
set /a TBuxs-=500
set /a ScaryFaceBuyed+= 1
goto ScaryFaceOwned1


:TRbxOrTBuxsScaryFace1
@echo off
cls
Color 80
Call Button [3] [3] [8F] ["Games"]
GetInput /M %_Box% /H %_hover:-0, -3% 70
Call Button [3] [3] [8F] ["Shop"] GetInput /M %_Box% /H %_hover:-0, -3% 70
Call Button [3] [3] [8F] ["Trading"]
GetInput /M %_Box% /H %_hover:-0, -3% 70
Call Button [3] [3] [8F] ["Casino"]
GetInput /M %_Box% /H %_hover:-0, -3% 70

echo call: F "Chose with Wath you want Buy super scary face" 

Call Button [3] [3] [8F] ["TRbx"]
GetInput /M %_Box% /H %_hover:-0, -3% 70 Call Button [3] [3] [8F] ["TBuxs"]
GetInput /M %_Box% /H %_hover:-0, -3% 70

If %errorlevel%==1 goto Games1
If %Errorlevel%==2 goto Shop1
If %Errorlevel%==3 goto Trading1
If %Errorlevel%==4 goto Casino1
If %Errorlevel%==5 goto BuyScaryFaceTRbx1
If %Errorlevel%==6 goto BuyScaryFaceTBuxs1


:ScaryFaceOwned1
@echo off
cls
Color 80
Call Button [3] [3] [8F] ["Games"]
GetInput /M %_Box% /H %_hover:-0, -3% 70
Call Button [3] [3] [8F] ["Shop"] GetInput /M %_Box% /H %_hover:-0, -3% 70
Call Button [3] [3] [8F] ["Trading"]
GetInput /M %_Box% /H %_hover:-0, -3% 70
Call Button [3] [3] [8F] ["Casino"]
GetInput /M %_Box% /H %_hover:-0, -3% 70

echo.
echo call: F "_______________"
echo call: F "| ---          ---         |        Scary Face"
echo call: F "|                           |"       Owned (1)
echo call: F "|     _________     |" 
echo call: F "|    |                |     |"
echo call: F "|     _________     |"
echo call: F "_______________"

If %errorlevel%==1 goto Games1
If %Errorlevel%==2 goto Shop1
If %Errorlevel%==3 goto Trading1
If %Errorlevel%==4 goto Casino1

:BuyFail1
@echo off
cls
color 80
Call Button [3] [3] [8F] ["Games"]
GetInput /M %_Box% /H %_hover:-0, -3% 70
Call Button [3] [3] [8F] ["Shop"] GetInput /M %_Box% /H %_hover:-0, -3% 70
Call Button [3] [3] [8F] ["Trading"]
GetInput /M %_Box% /H %_hover:-0, -3% 70
Call Button [3] [3] [8F] ["Casino"]
GetInput /M %_Box% /H %_hover:-0, -3% 70
echo call: F "Sorry but you don't have enough TBuxs/ TRbxs to buy this item/gamepass"

If %errorlevel%==1 goto Games1
If %Errorlevel%==2 goto Shop1
If %Errorlevel%==3 goto Trading1
If %Errorlevel%==4 goto Casino12
